{
  "name": "@neopilot-ai/cli",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "type": "module",
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=22"
  },
  "bin": {
    "neopilot": "dist/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "format": "prettier --check \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\" \"*.{json,md}\" .github/workflows/*.yml",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\" \"*.{json,md}\" .github/workflows/*.yml",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "pnpm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "pnpm run clean && node cli.mjs",
    "build:dev": "cross-env NODE_ENV=development node cli.mjs --dev && node --enable-source-maps dist/cli-dev.js",
    "check": "pnpm run format && pnpm run lint && pnpm run typecheck && pnpm test",
    "docker:build": "bash scripts/build_container.sh",
    "docker:run": "docker run -it --rm neopilot",
    "prebuild": "pnpm run clean",
    "build:dev:watch": "cross-env NODE_ENV=development tsc --watch & nodemon --watch dist/cli-dev.js --exec node --enable-source-maps dist/cli-dev.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "@inkjs/ui": "^2.0.0",
    "@types/express": "^5.0.1",
    "chalk": "^5.2.0",
    "diff": "^7.0.0",
    "express": "^5.1.0",
    "figures": "^6.1.0",
    "file-type": "^20.1.0",
    "ink": "^5.2.0",
    "ink-select-input": "^6.0.0",
    "js-yaml": "^4.1.1",
    "marked": "^15.0.7",
    "marked-terminal": "^7.3.0",
    "meow": "^13.2.0",
    "open": "^10.1.0",
    "openai": "^4.89.0",
    "react": "^18.2.0",
    "shell-quote": "^1.8.2",
    "terminal-image": "^3.1.1",
    "use-interval": "1.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/diff": "^7.0.2",
    "@types/js-yaml": "^4.0.9",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^20.14.10",
    "@types/react": "^18.0.32",
    "@types/shell-quote": "^1.7.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "cross-env": "^10.1.0",
    "esbuild": "^0.23.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "ink-testing-library": "^3.0.0",
    "nodemon": "^3.1.11",
    "prettier": "^2.8.7",
    "punycode": "^2.3.1",
    "strip-ansi": "^7.1.0",
    "ts-node": "^10.9.1",
    "type-fest": "^4.22.0",
    "typescript": "^5.5.0",
    "vitest": "^3.0.9",
    "whatwg-url": "^14.2.0"
  },
  "resolutions": {
    "braces": "^3.0.3",
    "micromatch": "^4.0.8",
    "semver": "^7.7.1"
  },
  "overrides": {
    "punycode": "^2.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neopilot-ai/neopilot-cli"
  }
}
